{
    "DetailedErrors": true,
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "DataStore": {
        "Type": "CosmosDb",
        "CosmosDb": {
            "EndpointUri": "https://dsr-dmcopilot-cdb.documents.azure.com:443/"
        }
    },
    "SemanticKernel": {
        "PluginsDirectory": "Plugins",
        "Type": "AzureOpenAI",
        "AzureOpenAiChatCompletionServices": [
            {
                "Id": "ChatCompletion",
                "Endpoint": "https://dsr-dmcopilot-oai.openai.azure.com/",
                "Deployment": "gpt-35-turbo",
                "SetAsDefault": true,
                "AlsoAsTextCompletion": true
            }
        ],
        "AzureOpenAiTextEmbeddingGenerationServices": [
            {
                "Id": "Embeddings",
                "Endpoint": "https://dsr-dmcopilot-oai.openai.azure.com/",
                "Deployment": "text-embedding-ada-002",
                "SetAsDefault": true
            }
        ],
        "AzureOpenAiImageServices": [
            {
                "Id": "ImageGeneration",
                "Endpoint": "https://dsr-dmcopilot-oai.openai.azure.com/",
                "SetAsDefault": true
            }
        ]
    },
    "AppConfiguration": {
        "Endpoint": "https://dsr-dmcopilot-appconfig.azconfig.io"
    }
}